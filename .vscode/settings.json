{
  "javascript.validate.enable": false,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "git.autofetch": false,
  "files.autoSave": "off",
  "files.exclude": {
    "**/dist": true
  },
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
